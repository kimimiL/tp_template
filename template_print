    <script>
        document.getElementById('btnPrint').onclick = function(){
            window.print();
        };

        var light_color = '#FFFFFF'
        var dark_color = '#000000'
        var logo_default = '#CB2026'

        // onchange event for background color picker
        function updateBGColor(picker){
            var parent_div = picker.closest(".feature-container")
            SVGSetAttr(parent_div.querySelectorAll('.svg-background'), 'fill', picker.value)
            if (isBright(picker.value)){
                SVGSetAttr(parent_div.querySelectorAll('.svg-logo-text'), 'fill', dark_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-logo-image'), 'fill', logo_default)
                SVGSetAttr(parent_div.querySelectorAll('.svg-outline'), 'stroke', dark_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-text'), 'fill', dark_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-inputs'), 'fill', dark_color)
            }else {
                SVGSetAttr(parent_div.querySelectorAll('.svg-logo-text'), 'fill', light_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-logo-image'), 'fill', light_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-outline'), 'stroke', light_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-text'), 'fill', light_color)
                SVGSetAttr(parent_div.querySelectorAll('.svg-inputs'), 'fill', light_color)
            }
        }
        // clone selected template
        function cloneFeature(elem){
            var x = elem.closest(".feature-container")
            var clone = x.cloneNode(true);
            document.getElementById("printDiv").appendChild(clone);
            updateCounter()
        }
        // remove template
        function removeFeature(elem){
            var count = document.getElementsByClassName("feature-container").length
            if (count > 1) {
                var parent_div = document.getElementById("printDiv");
                var child = elem.closest(".feature-container")
                parent_div.removeChild(child);
            }
            updateCounter()
        }
        // add new template
        function addFeature(elem){
            var clone = document.getElementById("template").cloneNode(true)
            clone.setAttribute("class", "feature-container");
            clone.setAttribute("id", "");
            clone.setAttribute("style", "");
            document.getElementById("printDiv").appendChild(clone);
            updateCounter()
        }
        // update total template counter
        function updateCounter() {
            var count = document.getElementsByClassName("feature-container").length
            var text = document.getElementById("counter")
            text.innerText = count + " template" + ((count > 1) ? "s " : " ") + "created"
        }
        // function to update svg input texts when user fills information 
        function updateSVGText(input){
            input.closest(".feature-container").querySelector("#svg-input-" + input.name).textContent = input.value
        }
        // function to update svg texts' font size based on font selector
        function updateFontSize(select){
            var parent_div = select.closest(".feature-container")
            var texts = parent_div.querySelectorAll('.svg-inputs')
            // prepare select html element for cloning
            for (var i = 0; i < select.options.length; i++) {
              select.options[i].removeAttribute('selected');
            }
            select.options[select.options.selectedIndex].setAttribute('selected', 'selected')
            // done
            SVGSetAttr(texts, 'font-size', select.value)
        }
        // sets the color of the svg object using either fill or stroke attribute
        function SVGSetAttr(object, attr, value){
            for (var i = 0; i < object.length; i++) {
              object[i].setAttribute(attr, value);
            }
        }
        // check if color is bright/
        function isBright(color){
            var rgb_color = hexToRgb(color)
            return Trendy.bright(rgb_color['r'],rgb_color['g'],rgb_color['b'])
        }
        // converts hex color to rgb
        function hexToRgb(hex) {
            var result = /^#?([a-f\d]{2})([a-f\d]{2})([a-f\d]{2})$/i.exec(hex);
            return result ? {
                r: parseInt(result[1], 16),
                g: parseInt(result[2], 16),
                b: parseInt(result[3], 16)
            } : null;
        }
    </script>

    <!-- start Trendy.js inline script -->
    <script>
        /** trendy **

        handy functions for every project
        by Jake Kara / TrendCT.org

        **/

        /* TODO move toProperCase and toBoolean out of the string hack */
        String.prototype.toProperCase = function(txt) {

            return this.toString().replace(/\w\S*/g, function(txt) {
                return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();
            });
        }

        /* Test if a string is 'true' or 'false' or unknown (-1)*/
        String.prototype.toBoolean = function () {

            if (this.toString() === "true") {
                return true;
            }
            else if (this.toString() === "false") {
                return false;
            }
            else {
                throw "Neither 'true' nor 'false'";
            }
        }

        function Trendy(){

            return this;
        }

        Trendy.getParam = function (param) {

            var sPageURL = decodeURIComponent(window.location.search.substring(1)),
                sURLVariables = sPageURL.split('&'),
                sParameterName,
                i;

            for (i = 0; i < sURLVariables.length; i++) {
                sParameterName = sURLVariables[i].split('=');

                if (sParameterName[0] === param) {
                    return sParameterName[1] === undefined ? true : sParameterName[1];
                }
            };

            return null;
        }

        // convert spaces to underscores
        Trendy.underscore = function(string) {
            return string.split(' ').join('_');
        }

        // conver underscores to spaces
        Trendy.space = function(string) {
            return string.split('_').join(' ');
        }

        // add commas to whole or decimal number
        Trendy.comma = function (val) {
            // make sure val is a string
            val = val.toString();

            // do nothing on non-numeric values
            if (isNaN(parseFloat(val))) {
            console.error("Trendy.comma: Value is not numeric.");
            return val;
            }
            // if there are more than two periods, it's not a valid number
            var dec_count = (val.match(/\./g) || []).length;
            if (dec_count > 1) {
                console.error("Trendy.comma: Value is not numeric (more than one decimal point: " + dec_count + ") ");
            return val
            }

            var valString = val.toString();
            var leftOfDecimal = null;
            var rightOfDecimal = null;

            // if decimal number, use recursion
            if (valString.indexOf(".") >= 0) {
                leftOfDecimal = valString.substring(0, valString.indexOf("."));
                rightOfDecimal = valString.substring(valString.indexOf(".") + 1);
                return this.comma(leftOfDecimal) + "." + rightOfDecimal;
            }

            // otherwise, just get to it
            while (/(\d+)(\d{3})/.test(val.toString())) {
                val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2');
            }

            return val;

        }

        // format for dollars
        Trendy.dollars = function(val) {

            //Limit to 2 decimal places
            val = val.toFixed(2);

            // format
            return ("$" + toString(this.comma(val)));
        }

        // big meaning we don't care about the pennies.
        Trendy.bigDollars = function(val)
        {
            // lop off decimal
            val = Math.round(parseFloat(val));

            // format
            return ("$" + this.comma(val));

        }

        Trendy.getObjectWithKeyValue = function(object, key, value)
        {
            var foundObject = false;
            $.each(object, function(index, object_item) {
                if (object_item.hasOwnProperty(key)){
                        if (object_item[key] == value){
                            foundObject = object_item;
                            return false;
                        }
                }
            });

            if (foundObject == false) {
                return false;
            }
            else {
                return foundObject;
            }

        }

        // based on brightness formula here http://www.w3.org/TR/AERT#color-contrast
        // ((Red value X 299) + (Green value X 587) + (Blue value X 114)) / 1000
        Trendy.brightness = function(r, g, b){
            return (r * 299 + g * 587 + b * 114) / 1000
        }

        // return true if r, g, b of color is brighter than x
        Trendy.brighterThan = function(r, g, b, x) {
            return (this.brightness(r,g,b) > x);
        }

        // return true if r, g, b is "bright" by arbitrary definition
        Trendy.bright = function (r, g, b) {
            return (this.brighterThan(r, g, b, 123));
        }

        Handy = new Trendy();
    </script>
    <!-- end Trendy.js inline script -->
    </body>
</html>
